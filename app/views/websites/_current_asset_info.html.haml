%div{id: "#{dom_id website}_current_asset_info"}
  = notice
  - Rails.logger.silence do
    %ul
      %li
        Unspidered: #{website.assets.where(status: "unspidered").count}
      %li
        ContentAsset: #{ContentAsset.publishable.count} of #{ContentAsset.count}
      - DataAsset.subclasses.sort_by(&:name).each do |subclass|
        %li
          #{subclass.name}: #{subclass.publishable.count} of #{subclass.count}
      - if asset.present?
        %li
          URL: #{asset.canonical_url}
